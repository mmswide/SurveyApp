<h1>My Events</h1>
<%= link_to('Create Festival', {:action => 'new'} , :class => 'btn btn-primary')%> <br>
<% @events.each do |event| %>
Festival Name : <%= event.event_name %>
	<%= link_to('View Tickets', {:controller => 'tickets', :event_id => event.id})%>
	<%= link_to('Show', {:action => 'show', :id => event.id})%>
	<%= link_to('edit', {:action => 'edit', :id => event.id}, :class => '')%>
	<%= link_to('delete', {:action => 'delete', :id => event.id}, :class => '')%> <br>
<% end %>


